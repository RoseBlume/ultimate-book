{
  "productName": "Ultimate-Code-Book",
  "version": "1.0.0-beta",
  "identifier": "io.github.roseblume.rosary",
  "build": {
    "beforeDevCommand": "pnpm run dev",
    "devUrl": "http://localhost:4321",
    "beforeBuildCommand": "pnpm run build",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "Ultimate Code Book",
        "label": "main",
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb","rpm", "nsis"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}